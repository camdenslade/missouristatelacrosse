option_settings:
  aws:elasticbeanstalk:application:environment:
    PORT: 8080
  aws:elasticbeanstalk:healthreporting:system:
    SystemType: enhanced
    HealthCheckSuccessThreshold: Ok
  aws:elb:healthcheck:
    HealthyThreshold: 2
    Interval: 10
    Timeout: 5
    UnhealthyThreshold: 3
    Target: HTTP:8080/actuator/health

